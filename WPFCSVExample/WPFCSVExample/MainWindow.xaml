<Window x:Class="WPFCSVExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFCSVExample"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="Grid">
        <Button x:Name="btnValidate" Content="Validate" HorizontalAlignment="Left" Margin="585,110,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label Content="Please Enter file path" HorizontalAlignment="Left" Margin="33,104,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtFilePath" HorizontalAlignment="Left" Height="23" Margin="184,107,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="379"/>
        <Button x:Name="btnProcess" Content="Process File" HorizontalAlignment="Left" Margin="33,167,0,0" VerticalAlignment="Top" Width="150" Height="55" IsEnabled="False" Click="btnProcess_Click"/>
        <ListBox x:Name="lstFIle" Margin="10,243,10,10"/>

    </Grid>
</Window>
